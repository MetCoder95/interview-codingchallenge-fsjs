{
  "name": "itexico-challenge",
  "version": "1.0.0",
  "description": "iTexito challenge",
  "main": "index.js",
  "scripts": {
    "seedDB": "node ./seed/seeder.js",
    "cleanDB": "node ./seed/clean.js",
    "clear": "rm -r ./dist || true",
    "build": "npm run clear && babel src -d dist",
    "build:docker-dev": "npm run build && docker build --target development -t iTexico/challenge ./",
    "build:docker": "npm run build && docker build -t iTexico/challenge ./",
    "start:docker-dev": "docker run -d -p 8000:8000 -it --rm --init iTexico/challenge || true",
    "start:local": "NODE_ENV=development nodemon ./src/index.js || true",
    "test": "NODE_ENV=test jest",
    "coverage": "NODE_ENV=test jest --coverage",
    "test:list": "NODE_ENV=test jest ./src/api/components/List/list.test.js",
    "coverage:list": "NODE_ENV=test jest ./src/api/components/List/list.test.js --coverage",
    "test:tag": "NODE_ENV=test jest ./src/api/components/Tags/tags.test.js",
    "coverage:tag": "NODE_ENV=test jest ./src/api/components/Tags/tags.test.js --coverage",
    "test:utils": "NODE_ENV=test jest ./src/api/components/utils/utils.test.js",
    "coverage:utils": "NODE_ENV=test jest ./src/api/components/utils/utils.test.js  --coverage"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MetCoder95/interview-codingchallenge-fsjs.git"
  },
  "author": "MetCoder95",
  "devDependencies": {
    "apidoc": "^0.17.7",
    "babel-jest": "^23.6.0",
    "bson-objectid": "^1.2.4",
    "faker": "^4.1.0",
    "jest": "^23.6.0",
    "jsdoc": "^3.5.5",
    "marked": "^0.3.17",
    "nodemon": "^1.18.8",
    "validator": "^10.9.0"
  },
  "dependencies": {
    "@sindresorhus/slugify": "^0.2.0",
    "axios": "^0.18.0",
    "babel-core": "^6.26.0",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "body-parser": "^1.18.2",
    "boom": "^7.3.0",
    "chalk": "^2.3.2",
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "csurf": "^1.9.0",
    "dotenv": "^5.0.1",
    "dotenv-safe": "^5.0.1",
    "express": "^4.16.4",
    "express-force-ssl": "^0.3.2",
    "express-rate-limit": "^2.14.2",
    "express-validator": "^5.3.0",
    "hashids": "^1.2.2",
    "helmet": "^3.15.0",
    "jsonwebtoken": "^8.4.0",
    "method-override": "^2.3.10",
    "mongoose": "^5.3.16",
    "mongoose-delete": "^0.4.0",
    "mongoose-keywords": "^0.3.2",
    "morgan": "^1.9.1",
    "ms": "^2.1.1",
    "querymen": "^2.1.3",
    "rotating-file-stream": "^1.3.9",
    "type-check": "^0.3.2"
  }
}
